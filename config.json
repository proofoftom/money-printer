{
  "MIN_TOKEN_AGE_SECONDS": 300,
  "MAX_ENTRY_MCAP_USD": 50000,
  "MIN_MCAP_POSITION": 0.001,
  "MAX_MCAP_POSITION": 0.01,
  "RISK_PER_TRADE": 0.05,
  "STOP_LOSS_PERCENT": 25,
  "TAKE_PROFIT_PERCENT": 30,
  "TRAILING_STOP_PERCENT": 20,
  "WS_URL": "wss://pumpportal.fun/api/data",
  "RECONNECT_INTERVAL": 5000,
  "LOGGING": {
    "NEW_TOKENS": false,
    "TRADES": false,
    "POSITIONS": true,
    "SAFETY_CHECKS": true
  },
  "SOL_USD_PRICE": 225,
  "MAX_TOKEN_AGE": 86400000,
  "MIN_LIQUIDITY_SOL": 1,
  "MIN_TOKEN_AGE": 60000,
  "MIN_HOLDER_COUNT": 10,
  "MIN_TRANSACTIONS": 5,
  "NOTIFICATIONS": {
    "POSITIONS": {
      "ENTRY": {
        "enabled": true,
        "sound": true,
        "minSize": 0.1
      },
      "EXIT": {
        "enabled": true,
        "sound": true,
        "minProfitPercent": 10,
        "minLossPercent": 5
      }
    },
    "SYSTEM": {
      "CONNECTION": {
        "enabled": true,
        "sound": false,
        "retryThreshold": 3
      },
      "SAFETY": {
        "enabled": true,
        "sound": true,
        "minSeverity": "medium"
      }
    },
    "PERFORMANCE": {
      "MILESTONES": {
        "enabled": true,
        "sound": true,
        "profitThresholds": [
          1,
          5,
          10
        ],
        "lossThresholds": [
          1,
          3,
          5
        ]
      },
      "DAILY_SUMMARY": {
        "enabled": true,
        "sound": false,
        "minTrades": 5
      }
    }
  },
  "KEYBOARD_SHORTCUTS": {
    "TRADING": {
      "PAUSE_RESUME": {
        "key": "space",
        "description": "Pause/Resume trading",
        "requiresConfirmation": false
      },
      "EMERGENCY_STOP": {
        "key": "escape",
        "description": "Emergency stop - close all positions",
        "requiresConfirmation": true
      }
    },
    "DISPLAY": {
      "CLEAR_SCREEN": {
        "key": "l",
        "ctrl": true,
        "description": "Clear screen"
      },
      "TOGGLE_AUTOSCROLL": {
        "key": "s",
        "ctrl": true,
        "description": "Toggle auto-scroll"
      },
      "TOGGLE_CHARTS": {
        "key": "c",
        "ctrl": true,
        "description": "Show/hide charts"
      }
    },
    "VIEWS": {
      "TRADE_HISTORY": {
        "key": "1",
        "description": "Show trade history"
      },
      "ACTIVE_POSITIONS": {
        "key": "2",
        "description": "Show active positions"
      },
      "PERFORMANCE": {
        "key": "3",
        "description": "Show performance stats"
      },
      "TOKEN_LIST": {
        "key": "4",
        "description": "Show token list"
      }
    },
    "QUICK_ACTIONS": {
      "INCREASE_RISK": {
        "key": "+",
        "description": "Increase risk per trade by 1%",
        "requiresConfirmation": true
      },
      "DECREASE_RISK": {
        "key": "-",
        "description": "Decrease risk per trade by 1%",
        "requiresConfirmation": true
      }
    }
  },
  "LOGGING_ENABLED": true,
  "LOG_LEVEL": "debug",
  "LOGGING_SETTINGS": {
    "MAX_SIZE": "20mb",
    "MAX_FILES": "14d"
  },
  "DATA_EXPORT": {
    "DIRECTORY": "./exports",
    "AUTO_EXPORT": {
      "enabled": true,
      "interval": 3600000,
      "types": [
        "trades",
        "performance",
        "tokens"
      ]
    },
    "FORMATS": {
      "CSV": {
        "enabled": true,
        "delimiter": ",",
        "includeHeaders": true
      },
      "JSON": {
        "enabled": true,
        "pretty": true
      }
    },
    "TRADES": {
      "enabled": true,
      "fields": [
        "timestamp",
        "token",
        "type",
        "price",
        "size",
        "value",
        "pnl",
        "pnlPercent",
        "holdTime",
        "exitReason"
      ],
      "groupBy": {
        "enabled": true,
        "intervals": [
          "hourly",
          "daily",
          "weekly"
        ]
      }
    },
    "PERFORMANCE": {
      "enabled": true,
      "fields": [
        "timestamp",
        "totalTrades",
        "winRate",
        "avgPnl",
        "maxDrawdown",
        "sharpeRatio",
        "profitFactor",
        "balance"
      ],
      "metrics": {
        "basic": true,
        "advanced": true,
        "custom": true
      }
    },
    "TOKENS": {
      "enabled": true,
      "fields": [
        "mint",
        "name",
        "symbol",
        "age",
        "marketCap",
        "volume",
        "trades",
        "holders"
      ],
      "filters": {
        "minAge": 300,
        "minMarketCap": 1000,
        "minTrades": 5
      }
    },
    "SYSTEM": {
      "enabled": true,
      "fields": [
        "timestamp",
        "event",
        "level",
        "message",
        "details"
      ],
      "levels": [
        "error",
        "warn",
        "info"
      ],
      "includeMetrics": true
    }
  },
  "autoExport": true,
  "autoExportInterval": 3600000,
  "customizeShortcuts": false
}